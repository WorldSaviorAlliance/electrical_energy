.eem_window_bg {
	background-color: #FFFFFF;
	position: fixed;
	top: 0px;
	left: 0px;
	opacity: 0;
	visibility: visible;
	display: block;
}

.eem_window_title {
	background: #2959A1;
	font-weight: bold;
	padding-left: 15px;
	color: #FFFFFF;
	height: 45px;
	line-height: 45px;
	box-sizing: initial;
}
.eem_window_close {
	margin: 5px;
	position: absolute;
	right: 0px;
	cursor: pointer;
	background-image: url('../../images/window/close_normal.png');
	width: 9px;
	height: 9px;	
	margin-top: 18px;
    margin-right: 15px;
}
.eem_window_close:hover {	
	background-image: url('../../images/window/close_click.png');
}
.eem_window_content {
	text-align: left;
	padding: 0px;
	overflow : hidden;
}

.eem_window_button_container {
	text-align: right;
	position: absolute;
	bottom: 0px;
	height: 50px;
	width: 100%;
	box-shadow: rgba(0, 0, 0, 0.2) 0px 4px 23px 5px, rgba(0, 0, 0, 0.14902) 0px 2px 6px;
	background-color: #f5f5f5;
}

.eem_window_button {
	margin: 10px;
	width: 60px;
	height: 25px;
	float: right;
}
